  "license": "MIT",
  "type": "module",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.modern.js",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "bundle:clean": "rimraf dist/**/*",
