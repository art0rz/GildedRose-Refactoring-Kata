  "devDependencies": {
    "microbundle": "^0.13.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "next": "11.1.2"
  },
  "dependencies": {
    "gilded-rose-lib": "1.0.0",
    "next-connect": "^0.10.2"
  }
